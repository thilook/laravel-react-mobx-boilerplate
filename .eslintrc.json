{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "parser": "babel-eslint",
  "rules": {
    "indent": ["error", 2],
    "func-names": ["error", "never"],
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "no-param-reassign": "off",
    "no-undef": "off"
  }
}